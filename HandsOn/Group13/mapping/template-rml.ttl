@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix vocab: <http://example.org#> .
@base <http://example.com/ns#>.

<#TriplesMap1> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "206974-0-agenda-eventos-culturales-100-updated.csv";
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rr:template "http://madrid.encuentra.es/resource/Evento/{id-evento}"; 
    rr:class esagendfa:Evento
  ];

  rr:predicateObjectMap [
    rr:predicate me:name;
    rr:objectMap [
      rml:reference "TITULO"
      ]
  ];

  rr:predicateObjectMap [
    rr:predicate me:description;
    rr:objectMap [
      rml:reference "DESCRIPCION";
      
      ]
  ];

  rr:predicateObjectMap [
    rr:predicate esAgenda:fechaInicio;
    rr:objectMap [
      rml:reference "FECHA";
      rr:datatype <http://www.w3.org/2001/XMLSchema#date>
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate esAgenda:fechaFin;
    rr:objectMap [
      rml:reference "FECHA-FIN";
      
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate esAgenda:horaInicio;
    rr:objectMap [
      rml:reference "HORA";
      
    ]
  ] .
<#TriplesMap2> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "PathToData.csv" ;
    rml:referenceFormulation ql:CSV 
  ];
  rr:subjectMap [
    rml:reference "columnReference";
    rr:class vocab:MyClass2
  ].
